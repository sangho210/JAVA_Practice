class Test2 {

	public static void main(String[] args) {

		int a = 10; //자료형 int(integer)
		int b = 5;

		int c = a + b;
		int d = a - b;

		System.out.println(a + "+" +  b + "=" + c);//println, 줄바꿈.
		System.out.printf("%d-%d=%d\n", a, b, d);//printf, %d 사용, 줄바꿈 없음.
		//System.out.println();


	}
}